<form
  name="formProducts"
  id="formProducts"
  class="row needs-validation mt-2 mx-2"
  enctype="multipart/form-data"
  novalidate
>
  <div class="col-6">
    <input name="id" type="hidden" id="id" />
    <div class="col-md">
      <label for="title" class="form-label">Título del producto</label>
      <input
        name="title"
        class="form-control"
        id="title"
        placeholder="Ingrese un título"
        required
      />
      <div class="invalid-feedback">
        Ingrese un título.
      </div>
    </div>
    <div class="col-md">
      <label for="description" class="form-label mt-3">Descripción</label>
      <input
        name="description"
        type="text-area"
        class="form-control"
        id="description"
        placeholder="Ingrese una descripción"
        required
      />
      <div class="invalid-feedback">
        Ingrese una descripción.
      </div>
    </div>
    <div class="col-md">
      <label for="price" class="form-label mt-3">Precio</label>
      <input
        name="price"
        type="number"
        class="form-control"
        id="price"
        placeholder="Ingrese el precio"
        required
      />
      <div class="invalid-feedback">
        Ingrese el precio.
      </div>
    </div>
    <div class="col-md">
      <label for="code" class="form-label mt-3">Codigo</label>
      <input
        name="code"
        type="text"
        class="form-control"
        id="code"
        placeholder="Ingrese el código"
        required
      />
      <div class="invalid-feedback">
        Ingrese un código de producto.
      </div>
    </div>
    <div class="col-md">
      <label for="category" class="form-label mt-3">Categoria</label>
      <select
        name="category"
        type="text"
        class="form-control"
        id="category"
        placeholder="Ingrese la categoria"
        required
      >
        <option value="" disabled selected>Selecciona una categoria</option>
        <option name="category" id="category" value="pc">PC</option>
        <option name="category" id="category" value="monitor">Monitor</option>
        <option name="category" id="category" value="teclado">Teclado</option>
      </select>
      <div class="invalid-feedback">
        Seleccione la categoria.
      </div>
    </div>
    <div class="col-md">
      <label for="stock" class="form-label mt-3">Stock</label>
      <input
        name="stock"
        type="number"
        class="form-control"
        id="stock"
        placeholder="Ingrese la cantidad"
        required
      />
      <div class="invalid-feedback">
        Ingrese la cantidad.
      </div>
    </div>
  </div>
  <div class="col-6">
      <label for="thumbnail" class="form-label">Imagenes (Campo no obligatorio)</label>
      <input
        name="thumbnail"
        type="file"
        class="form-control"
        id="thumbnail"
        placeholder="Ingrese URL de las imagenes"
        {{!-- required --}}
      />
      {{!-- <div class="invalid-feedback">
        Please provide a valid zip.
      </div> --}}
    
      <button
        class="btn btn-success mt-3"
        type="submit"
        name="submit"
        id="submit"
      >Guardar producto</button>
  </div>
</form>

